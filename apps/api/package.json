{
  "name": "pro-chat-api",
  "private": true,
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "dev": "DATABASE_URL=file:./data/pro-chat.db node ../../node_modules/tsx/dist/cli.mjs watch src/index.ts",
    "build": "node ../../node_modules/typescript/lib/tsc.js -p tsconfig.build.json",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "prisma:generate": "DOTENV_CONFIG_PATH=../../.env node -r dotenv/config ../../node_modules/prisma/build/index.js generate",
    "prisma:migrate": "DOTENV_CONFIG_PATH=../../.env node -r dotenv/config ../../node_modules/prisma/build/index.js db push",
    "prisma:studio": "DOTENV_CONFIG_PATH=../../.env node -r dotenv/config ../../node_modules/prisma/build/index.js studio"
  },
  "dependencies": {
    "@prisma/client": "^5.12.0",
    "dotenv": "^16.4.5",
    "eventsource-parser": "^1.1.0",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.4",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/pdf-parse": "^1.1.5",
    "prisma": "^5.12.0",
    "supertest": "^6.3.4",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  }
}
