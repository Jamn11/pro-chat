# OpenRouter + Brave keys are configured in the app Settings (BYOK)
# Optional metadata passed to OpenRouter
OPENROUTER_APP_URL=http://localhost:5173
OPENROUTER_APP_NAME=pro-chat

# SQLite DB path is resolved relative to apps/api/prisma
DATABASE_URL=file:./data/pro-chat.db
PORT=8787
STORAGE_PATH=storage
MEMORY_PATH=memory
